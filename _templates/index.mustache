---
layout: default
title: "JW Developer - JW Player Demos"
published: true
---

{% include components/site-secondary-header/site-secondary-header.html product="player" %}

<div class="demos">

  {{> demos-index-header}}

  <div class="demo-index demo-index-{{indexType}}">

    <ul class="demo-index-nav">
      {{#categories}}
        <li><a href="{{&path.href}}{{#directory}}{{directory}}{{/directory}}" {{#current}} class="selected"{{/current}}>{{name}}</a></li>
      {{/categories}}
    </ul>

		<select class="demo-index-nav-select">
			{{#categories}}
        <option {{#current}}selected="selected"{{/current}} value="{{#directory}}{{directory}}{{/directory}}">{{name}}</option>
      {{/categories}}
		</select>

    <p class="demo-index-desc">
      {{#categories}}
        {{{currentDescription}}}
      {{/categories}}
    </p>

    <p class="demo-index-desc">Don't have an account? &nbsp;<a class="btn sm red" href="https://www.jwplayer.com/pricing">Sign up</a></p>

    <ul class="demo-index-list">
      {{#demos}}
        <li>
          <a href="{{category.directory}}/{{directory}}">
            <div class="header {{category.directory}}">
              {{category.name}}
            </div>
            <div class="body">
              <h3>{{title}}</h3>
              <p>{{description}}</p>
            </div>
          </a>
        </li>
      {{/demos}}
    </ul>

  </div>

</div>

{{> document-bottom}}